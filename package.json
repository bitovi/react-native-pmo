{
  "name": "PlaceMyOrder",
  "version": "1.0.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "eslint": "eslint *.ts *.cjs src",
    "eslint:fix": "eslint --fix *.ts *.cjs src",
    "prettier": "prettier --check *.ts *.cjs src",
    "prettier:fix": "prettier --write *.ts *.cjs src",
    "depcheck": "depcheck .",
    "precheck": "npm run typecheck && npm run eslint && npm run prettier && npm run depcheck && npm run test",
    "start": "react-native start --reset-cache",
    "android": "react-native run-android",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "clean": "rm -rf tsconfig.tsbuildinfo coverage android/.gradle android/build android/app/build node_modules/.cache",
    "devtools": "react-devtools",
    "api": "npm run linkLocalhost:android && place-my-order-api --port 7070",
    "linkLocalhost:android": "adb reverse tcp:7070 tcp:7070"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@types/jest": "^29.5.12",
    "expo": "~50.0.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-expo": "~50.0.2",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-maps": "^1.13.0",
    "react-native-screens": "^3.30.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@bitovi/eslint-config": "^1.8.0",
    "@react-native/metro-config": "0.73.5",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "~18.2.45",
    "@types/react-native-vector-icons": "^6.4.18",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "place-my-order-api": "^1.7.0",
    "prettier": "^3.2.5",
    "react-devtools": "^5.0.2",
    "typescript": "^5.1.3"
  }
}
