{
  "name": "PlaceMyOrder",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "eslint": "eslint *.ts *.js src",
    "prettier": "prettier --check *.ts *.js src",
    "depcheck": "depcheck .",
    "ci": "npm run typecheck && npm run eslint && npm run prettier && npm run test && npm run depcheck",
    "start": "expo start",
    "android": "react-native run-android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf tsconfig.tsbuildinfo .expo dist ios android coverage"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@bitovi/eslint-config/react",
    "settings": {
      "import/ignore": [
        "node_modules/react-native/index\\.js$"
      ]
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleNameMapper": {
      "\\.(jpg)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-navigation/bottom-tabs": "^7.0.0-alpha.17",
    "@react-navigation/native": "^7.0.0-alpha.14",
    "@react-navigation/native-stack": "^7.0.0-alpha.15",
    "@types/jest": "^29.5.12",
    "expo": "~50.0.8",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.9",
    "expo-linking": "~6.2.2",
    "expo-router": "^3.4.8",
    "expo-status-bar": "~1.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-expo": "~50.0.2",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.30.1",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@bitovi/eslint-config": "^1.7.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/metro-config": "0.73.5",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "~18.2.45",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.1.3"
  }
}
