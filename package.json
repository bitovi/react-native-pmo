{
  "name": "PlaceMyOrder",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "api": "place-my-order-api --port 7070",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint *.ts *.js src",
    "prettier": "prettier --check *.ts *.js src",
    "depcheck": "depcheck .",
    "precheck": "npm run typecheck && npm run eslint && npm run prettier && npm run test && npm run depcheck",
    "start": "react-native start",
    "android": "react-native run-android",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "clean": "rm -rf tsconfig.tsbuildinfo coverage android/.gradle android/build android/app/build",
    "react-devtools": "react-devtools"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@bitovi/eslint-config/react",
    "settings": {
      "import/ignore": [
        "node_modules/react-native/index\\.js$"
      ]
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "./node_modules/@react-native-google-signin/google-signin/jest/build/setup.js",
      "<rootDir>/jest-setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "transform": {}
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-navigation/bottom-tabs": "^7.0.0-alpha.17",
    "@react-navigation/native": "^7.0.0-alpha.14",
    "@react-navigation/native-stack": "^7.0.0-alpha.15",
    "@types/jest": "^29.5.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "place-my-order-api": "^1.7.0",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-dotenv": "^3.4.11",
    "react-native-maps": "^1.11.3",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.30.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@bitovi/eslint-config": "^1.7.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/metro-config": "0.73.5",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "~18.2.45",
    "@types/react-native-vector-icons": "^6.4.18",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "react-devtools": "^5.0.2",
    "typescript": "^5.1.3"
  }
}