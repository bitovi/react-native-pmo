{
  "name": "react-native-pmo",
  "version": "1.0.0",
  "private": true,
  "main": "App.tsx",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "eslint": "eslint *.ts *.js src",
    "prettier": "prettier --check *.ts *.js src",
    "depcheck": "depcheck .",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf tsconfig.tsbuildinfo dist coverage",
    "build": "echo 'build'"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@bitovi/eslint-config/react",
    "settings": {
      "import/ignore": ["node_modules/react-native/index\\.js$"]
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "dependencies": {
    "@react-navigation/native": "^7.0.0-alpha.11",
    "@react-navigation/native-stack": "^7.0.0-alpha.12",
    "@types/jest": "^29.5.12",
    "expo": "~50.0.8",
    "expo-constants": "~15.4.5",
    "expo-linking": "~6.2.2",
    "expo-status-bar": "~1.11.1",
    "jest": "^29.3.1",
    "jest-expo": "~50.0.2",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@bitovi/eslint-config": "^1.7.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "~18.2.45",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.1.3"
  }
}
